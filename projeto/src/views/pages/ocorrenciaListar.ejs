                                                                        <a class="nav-link" href="#"  onclick="emitirParecerShowModal(<%=ocorrencia.idOcorrencia%>)">
                                                                            <i class="fa-solid fa-user-pen fa-lg"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                    <%
                                                            }
                                                            
                                                            idOcorrencia = ocorrencia.idOcorrencia;
                                                        });
    
